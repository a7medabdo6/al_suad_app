{
  "name": "realestate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "bundle:release": "cd android && ./gradlew bundleRelease && cd ..",
    "assemble:release": "cd android && ./gradlew assembleRelease && cd ..",
    "assemble:debug": "cd android && ./gradlew assembleDebug && cd ..",
    "test": "jest",
    "elint": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./",
    "pod:install": "cd ios && pod install --repo-update && cd ..",
    "prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/BuzzbotRNApp/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.5",
    "@react-navigation/bottom-tabs": "^6.5.1",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/stack": "^6.3.10",
    "native-base": "^3.4.25",
    "react": "18.1.0",
    "react-native": "0.69.6",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-maps": "^1.3.2",
    "react-native-pager-view": "^6.1.2",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-reanimated": "^2.13.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.3.4",
    "react-native-vector-icons": "^9.2.0",
    "reanimated-bottom-sheet": "^1.0.0-alpha.22"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
